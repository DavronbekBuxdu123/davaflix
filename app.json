{
  "expo": {
    "name": "DavaFlix",
    "slug": "Movie-appp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconic.png",
    "scheme": "davaflix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconic.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dava077.davaflix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/iconic.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/iconic.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ee6dfbe-b453-4faf-a140-5d28746f02ac"
      }
    },
    "owner": "dava077"
  }
}
